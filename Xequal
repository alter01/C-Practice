#include <stdio.h>

int main(void){
  printf("This program will test if X is less than, equal to, or more than Y.\n");
 int x = 5, y = 5;


if (x < y)

    {
        printf ("Y is less than X.\n");
    }
    
if (x == y)

    {    
        printf ("Y is equal to X.\n");
    }
    
else

    {
        printf ("X is more than Y.\n");
    }
    
printf ("That's it, all done. Goodbye!\n");
}
